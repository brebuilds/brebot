{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../static",
    "distDir": "../static"
  },
  "package": {
    "productName": "Brebot Desktop",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "open": true
      },
      "http": {
        "all": true,
        "request": true
      }
    },
    "bundle": {
      "active": true,
      "identifier": "com.brebot.desktop",
      "icon": [
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/icon.png",
        "icons/icon-1024.png",
        "icons/icon-512.png",
        "icons/icon-256.png",
        "icons/icon-128.png",
        "icons/icon-64.png",
        "icons/icon-32.png",
        "icons/icon-16.png"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "label": "main",
        "title": "Brebot Desktop",
        "fullscreen": false,
        "width": 1280,
        "height": 800,
        "resizable": true,
        "center": true
      }
    ]
  }
}
